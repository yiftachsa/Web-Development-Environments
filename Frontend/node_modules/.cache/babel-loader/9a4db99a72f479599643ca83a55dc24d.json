/mealPlan/addRecipe/recipeID/${this.recipe}`\r\n          );\r\n          if (response.status == 200) {\r\n            //added\r\n            this.counter++;\r\n            this.message = \"recipe is being added to meal plan\";\r\n            this.clicked = true;\r\n            this.$emit(\"updateSize\");\r\n          } else {\r\n            console.log(error);\r\n          }\r\n        } catch (error) {\r\n          let toString = error + \"\";\r\n          if (toString.includes(\"409\")) {\r\n            //already in meal\r\n            this.counter++;\r\n            this.message = \"recipe already in meal plan\";\r\n            this.clicked = true;\r\n          } else {\r\n            console.log(error);\r\n          }\r\n        }\r\n      } else {\r\n        this.$router.push(\"/\").catch(() => {\r\n          this.$forceUpdate();\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n>\r\n"],"sourceRoot":"src/components"}]}