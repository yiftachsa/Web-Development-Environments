,IAdA,kBAcA;AACA,WAAA;AACA,MAAA,QAAA,EAAA;AADA,KAAA;AAGA,GAlBA;AAmBA,EAAA,QAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,UAAA,KAAA,GAAA,CAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,oBAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,oBAAA,CAAA,CAAA,EAAA,KAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,UAAA,KAAA;AACA;AACA;;AACA,UAAA,QAAA,CAAA,GAAA,CAAA,SAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,0BAAA,CAAA,KAAA,QAAA,EAAA,KAAA;;AACA,YAAA,CAAA,KAAA,KAAA,CAAA,KAAA,CAAA,iBAAA,CAAA,KAAA,QAAA,CAAA,EAAA;AACA,eAAA,KAAA,CAAA,KAAA,CAAA,qBAAA,CAAA,KAAA,QAAA,EAAA,CAAA;AACA;AACA;;AACA,aAAA,KAAA;AACA,KAfA;AAgBA,IAAA,QAhBA,sBAgBA;AACA,UAAA,KAAA,QAAA,GAAA,KAAA,aAAA,EAAA;AACA,eAAA,KAAA;AACA,OAFA,MAEA;AACA,eAAA,IAAA;AACA;AACA;AAtBA,GAnBA;AA2CA,EAAA,OA3CA,qBA2CA;AACA,SAAA,YAAA;AACA,GA7CA;AA8CA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA,UAAA,eAAA,GAAA,CAAA;;AACA,UAAA,KAAA,gBAAA,EAAA;AACA,2CAAA,MAAA,CAAA,OAAA,CAAA,KAAA,gBAAA,CAAA,qCAAA;AAAA;AAAA,cAAA,GAAA;AAAA,cAAA,KAAA;;AACA,cAAA,QAAA,GAAA,QAAA,CAAA,cAAA,mBAAA,GAAA,EAAA,CADA,CACA;;AACA,UAAA,QAAA,CAAA,OAAA,GAAA,KAAA,CAFA,CAEA;;AACA,cAAA,KAAA,EAAA;AACA,YAAA,eAAA;AACA;AACA;AACA;;AACA,WAAA,QAAA,GAAA,eAAA;AACA,KAbA;AAcA,IAAA,OAdA,mBAcA,YAdA,EAcA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,cAAA,mBAAA,YAAA,CAAA,MAAA,EAAA;;AACA,UAAA,QAAA,CAAA,OAAA,IAAA,IAAA,EAAA;AACA,aAAA,QAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,qBAAA,CAAA,KAAA,QAAA,EAAA,KAAA,QAAA;AACA,OAHA,MAGA;AACA,aAAA,QAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,qBAAA,CAAA,KAAA,QAAA,EAAA,KAAA,QAAA;AACA;;AACA,UAAA,YAAA,GAAA;AACA,QAAA,UAAA,EAAA,YAAA,CAAA,MADA;AAEA,QAAA,SAAA,EAAA,QAAA,CAAA;AAFA,OAAA;AAIA,WAAA,KAAA,CAAA,kBAAA,EAAA,YAAA;AACA;AA5BA;AA9CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <b-container>\r\n      <div class=\"textBold\">\r\n        number Of Steps {{ numberOfSteps }} progress {{ progress }} finished\r\n        {{ finished }}\r\n      </div>\r\n      <b-progress :max=\"numberOfSteps\" animated>\r\n        <b-progress-bar\r\n          :value=\"progress\"\r\n          :label=\"`${progress} steps of ${numberOfSteps} completed`\"\r\n          v-if=\"!finished\"\r\n        ></b-progress-bar>\r\n        <b-progress-bar\r\n          :value=\"progress\"\r\n          label=\"Enjoy your meal\"\r\n          v-else\r\n        ></b-progress-bar>\r\n      </b-progress>\r\n      <h5>check every step you complete to track your progress</h5>\r\n      <div v-for=\"detailedIns in detailedInstructions\" :key=\"detailedIns.id\">\r\n        <b-card v-for=\"step in detailedIns.steps\" :key=\"step.id\">\r\n          <input\r\n            type=\"checkbox\"\r\n            :id=\"'checkBox' + step.number\"\r\n            @click=\"checked(step)\"\r\n          />\r\n\r\n          {{ step.number }})\r\n\r\n          {{ step.step }}\r\n          <div v-if=\"step.step_equipment.length > 0\">\r\n            <strong class=\"bold\">Equipment:</strong>\r\n            <ul>\r\n              <li v-for=\"equipment in step.step_equipment\" :key=\"equipment.id\">\r\n                {{ equipment }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <div v-if=\"step.step_ingredients.length > 0\">\r\n            <strong class=\"bold\">Ingredients:</strong>\r\n            <ul>\r\n              <li\r\n                v-for=\"ingredient in step.step_ingredients\"\r\n                :key=\"ingredient.id\"\r\n              >\r\n                {{ ingredient }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </b-card>\r\n      </div>\r\n    </b-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    detailedInstructions: {\r\n      type: Array,\r\n      requierd: true,\r\n    },\r\n    previousProgress: {\r\n      type: Object,\r\n      requierd: false,\r\n    },\r\n    recipeId: {\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      progress: 0,\r\n    };\r\n  },\r\n  computed: {\r\n    numberOfSteps() {\r\n      let count = 0;\r\n      for (let i = 0; i < this.detailedInstructions.length; i++) {\r\n        for (let j = 0; j < this.detailedInstructions[i].steps.length; j++) {\r\n          count++;\r\n        }\r\n      }\r\n      if ($cookies.get(\"session\")) {\r\n        this.$root.store.updateNumberOfInstructions(this.recipeId, count);\r\n        if (!this.$root.store.isAlreadyProgress(this.recipeId)) {\r\n          this.$root.store.updateIntegerProgress(this.recipeId, 0);\r\n        }\r\n      }\r\n      return count;\r\n    },\r\n    finished() {\r\n      if (this.progress < this.numberOfSteps) {\r\n        return false;\r\n      } else {\r\n        return true;\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.initProgress();\r\n  },\r\n  methods: {\r\n    initProgress() {\r\n      let currentProgress = 0;\r\n      if (this.previousProgress) {\r\n        for (const [key, value] of Object.entries(this.previousProgress)) {\r\n          let checkBox = document.getElementById(`checkBox${key}`); //key is stepNumber\r\n          checkBox.checked = value; //value is isChecked\r\n          if (value) {\r\n            currentProgress++;\r\n          }\r\n        }\r\n      }\r\n      this.progress = currentProgress;\r\n    },\r\n    checked(checkBoxStep) {\r\n      let checkBox = document.getElementById(`checkBox${checkBoxStep.number}`);\r\n      if (checkBox.checked == true) {\r\n        this.progress++;\r\n        this.$root.store.updateIntegerProgress(this.recipeId, this.progress);\r\n      } else {\r\n        this.progress--;\r\n        this.$root.store.updateIntegerProgress(this.recipeId, this.progress);\r\n      }\r\n      let progressInfo = {\r\n        stepNumber: checkBoxStep.number,\r\n        isChecked: checkBox.checked,\r\n      };\r\n      this.$emit(\"progress-changed\", progressInfo);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.bold {\r\n  font-weight: 600;\r\n  font-size: 1.5rem;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}