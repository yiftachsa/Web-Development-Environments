\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      let response_1;\r\n      // response = this.$route.params.response;\r\n\r\n      try {\r\n        response_1 = await this.axios.get(\r\n          \"https://assignment-3-2-merav-yiftach.herokuapp.com/recipes/recipeID/\" +\r\n            this.$route.params.recipeId,\r\n          { withCredentials: true }\r\n        );\r\n\r\n        // console.log(\"response.status\", response.status);\r\n        if (response_1.status !== 200) this.$router.replace(\"/NotFound\");\r\n      } catch (error) {\r\n        console.log(\"error.response\", error.response_1);\r\n        this.$router.replace(\"/NotFound\");\r\n        return;\r\n      }\r\n\r\n      let { summary, instructions, yeild, ingredients } = response_1.data;\r\n\r\n      let {\r\n        recipeId,\r\n        photo,\r\n        title,\r\n        time,\r\n        likes,\r\n        veganVegetarian,\r\n        glutenFree\r\n      } = response_1.data.preview;\r\n\r\n      let _recipe = {\r\n        recipeId,\r\n        photo,\r\n        title,\r\n        time,\r\n        summary,\r\n        likes,\r\n        veganVegetarian,\r\n        glutenFree,\r\n        instructions,\r\n        ingredients,\r\n        photo,\r\n        title,\r\n        yeild\r\n      };\r\n\r\n      this.recipe = _recipe;\r\n    } catch (error) {\r\n      console.log(error);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.wrapper {\r\n  display: flex;\r\n}\r\n\r\n.wrapped {\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  margin-right: 4%;\r\n}\r\n.center {\r\n  display: block;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n  width: 50%;\r\n}\r\n\r\n.strongTitle {\r\n  position: relative;\r\n  font-family: \"Dosis\", sans-serif;\r\n  max-width: 800px;\r\n  margin: 0 5% 1rem;\r\n  font-size: 2.5rem;\r\n  font-weight: 800;\r\n  color: black;\r\n  background-image: url(\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/664131/underline.svg\");\r\n  backgrou