n\">\r\n      <b-form-group\r\n        id=\"input-group-Username\"\r\n        label-cols-sm=\"3\"\r\n        label=\"Username:\"\r\n        label-for=\"Username\"\r\n      >\r\n        <b-form-input\r\n          id=\"Username\"\r\n          v-model=\"$v.form.username.$model\"\r\n          type=\"text\"\r\n          :state=\"validateState('username')\"\r\n        ></b-form-input>\r\n        <b-form-invalid-feedback v-if=\"!$v.form.username.required\">\r\n          Username is required\r\n        </b-form-invalid-feedback>\r\n        <b-form-invalid-feedback v-else-if=\"!$v.form.username.length\">\r\n          Username length must be between 3-8 characters long\r\n        </b-form-invalid-feedback>\r\n        <b-form-invalid-feedback v-if=\"!$v.form.username.alpha\">\r\n          Username must contain only letters\r\n        </b-form-invalid-feedback>\r\n      </b-form-group>\r\n\r\n      <b-form-group\r\n        id=\"input-group-Password\"\r\n        label-cols-sm=\"3\"\r\n        label=\"Password:\"\r\n        label-for=\"Password\"\r\n      >\r\n        <b-form-input\r\n          id=\"Password\"\r\n          type=\"password\"\r\n          v-model=\"$v.form.password.$model\"\r\n          :state=\"validateState('password')\"\r\n        ></b-form-input>\r\n        <b-form-invalid-feedback>\r\n          Password is required\r\n        </b-form-invalid-feedback>\r\n      </b-form-group>\r\n\r\n      <b-button\r\n        type=\"submit\"\r\n        variant=\"info\"\r\n        style=\"width:100px;display:block;\"\r\n        class=\"mx-auto w-100\"\r\n        >Login</b-button\r\n      >\r\n      <div class=\"mt-2\">\r\n        Do not have an account yet?\r\n        <router-link to=\"register\"> Register here</router-link>\r\n      </div>\r\n    </b-form>\r\n    <b-alert\r\n      class=\"mt-2\"\r\n      v-if=\"form.submitError\"\r\n      variant=\"warning\"\r\n      dismissible\r\n      show\r\n    >\r\n      Login failed: {{ form.submitError }}\r\n    </b-alert>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  required,\r\n  minLength,\r\n  maxLength,\r\n  alpha,\r\n} from \"vuelidate/lib/validators\";\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        username: \"\",\r\n        password: \"\",\r\n        submitError: undefined,\r\n      },\r\n    };\r\n  },\r\n  validations: {\r\n    form: {\r\n      username: {\r\n        required,\r\n        length: (u) => minLength(3)(u) && maxLength(8)(u),\r\n        alpha,\r\n      },\r\n      password: {\r\n        required,\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    validateState(param) {\r\n      const { $dirty, $error } = this.$v.form[param];\r\n      return $dirty ? !$error : null;\r\n    },\r\n    async Login() {\r\n      try {\r\n        this.$root.store.ClearStorage();\r\n        const response = await this.axios.post(\r\n          `${this.$root.store.baseUrl}/login`,\r\n          {\r\n            username: this.form.username,\r\n            password: this.form.password,\r\n          }\r\n        );\r\n        const response_photo = await this.axios.get(\r\n          `${this.$root.store.baseUrl}/user/photoLink`\r\n        );\r\n        this.$root.store.login(\r\n          this.form.username,\r\n          response_photo.data.photoLink\r\n        );\r\n        this.$emit(\"updateSize\");\r\n        this.$router.push(\"/\").catch(() => {\r\n          this.$forceUpdate();\r\n        });\r\n        this.forceRender();\r\n      } catch (err) {\r\n        if (err.response) {\r\n          console.log(err.response);\r\n          this.form.submitError = err.response.data.message;\r\n        } else {\r\n          console.log(err);\r\n        }\r\n      }\r\n    },\r\n    onLogin() {\r\n      this.form.submitError = undefined;\r\n      this.$v.form.$touch();\r\n      if (this.$v.form.$anyError) {\r\n        return;\r\n      }\r\n      this.Login();\r\n    },\r\n    forceRender() {\r\n      this.$emit(\"rerenderLogout\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}